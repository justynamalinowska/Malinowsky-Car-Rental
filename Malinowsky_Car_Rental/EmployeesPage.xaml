<Window x:Class="Malinowsky_Car_Rental.EmployeesPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Malinowsky_Car_Rental"
        mc:Ignorable="d"
        Title="Employees Page" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" FontSize="15" Height="300" Width="500" Icon="/Images/Icon.png" >
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label Content="Base Id" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <TextBox x:Name="txtBaseId" Grid.Column="1" Width="120" HorizontalAlignment="Left" VerticalAlignment="Center"/>

    </Grid>
</Window>
